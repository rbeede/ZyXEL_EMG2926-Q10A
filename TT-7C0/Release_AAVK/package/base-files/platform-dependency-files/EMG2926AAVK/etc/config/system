# System Mode:1:Router,2:AP,3:Repeater,4:WISP,5:WISP+UR

config system main
	option product_name EMG2926-Q10A
	option product_model DUAL_BAND
	option hostname	EMG2926-Q10A
	option weather_city TWXX0021
	option weather_degree c
	option domain_name local
	option pwd 1234
	option power_saving_select '2.4G'
	option language "en"
	option systimeout 300
	option klogconloglevel 1
	option log_size 128
	option timezone UTC
	option system_mode 1
	option eaZy123	'1'

config rdate
	option interface wan	

config samba general
	option enable 0
	option usb1_types 0
	option usb2_types 0

config samba_user_config samba_user_1
	option enable 0
	option name ""
	option passwd ""
	option enable_usb1 0
	option enable_usb2 0

