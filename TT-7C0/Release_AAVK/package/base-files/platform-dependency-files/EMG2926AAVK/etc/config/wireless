config wifi-iface 'ath3'
	option device 'wifi0'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option RekeyInterval '3600'
	option WPAPSKCompatible '0'
	option PassPhrase 'avc'
	option key2 'fd78c70966'
	option key3 'd429c1dfc3'
	option key4 '8f39be9025'
	option key '1'
	option RADIUS_Server '192.168.2.3'
	option RADIUS_Port '1812'
	option session_timeout_interval '3666'
	option RADIUS_Key '12345'
	option keytype '0'
	option key1 '11111'
	option guest_ip_mask '255.255.255.0'
	option wepencryp128 'WEP64_128'
	option guest_ip '192.168.2.1'
	option guest_bandwidth_enable '0'
	option guest_max_bandwidth '256'
	option IntraBSS '1'
	option wmm '1'
	option auth 'OPEN'
	option encryption 'NONE'
	option enable_guest_wlan '0'
	option ssid 'ZyXEL_SSID3'
	option disabled '1'
	option shortgi '1'
	option group 'br-lan'
	option pmf '0'

