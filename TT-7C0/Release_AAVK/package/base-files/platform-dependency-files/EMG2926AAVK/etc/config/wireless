	option IntraBSS '1'
	option auth 'OPEN'
	option encryption 'NONE'
	option hidden '0'
	option wmm '1'
	option ssid 'ZyXEL_SSID1'
	option disabled '1'
	option shortgi '1'
	option group 'br-lan'
	option pmf '0'

