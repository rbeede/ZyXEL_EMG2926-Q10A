config WiFi wifi
	option button_useful '1'

config wifi-device 'wifi0'
	option type 'ar71xx'
	option country 'US'
	option encryption 'NONE'
	option channel_ext '0'
	option auth 'OPEN'
	option rts '2346'
	option frag '2346'
	option AutoChannelSelect '1'
	option txpower '100'
	option disabled '0'
	option channel_width 'Auto'
	option hwmode '11bgn'
	option channel '1'

config wifi-iface 'ath0'
	option device 'wifi0'
	option network 'lan'
	option mode 'ap'
	option RekeyInterval '3600'
	option WPAPSKCompatible '0'
	option PassPhrase 'avc'
	option RADIUS_Server '192.168.2.3'
	option RADIUS_Port '1812'
	option session_timeout_interval '0'
	option RADIUS_Key '12345'
	option PreAuth '0'
	option PMKCachePeriod '10'
	option hidden '0'
	option keytype '0'
	option key1 '11111'
	option key2 '22222'
	option key3 '33333'
	option key4 '44444'
	option key '1'
	option wmm '1'
	option auth 'WPA2PSK'
	option encryption 'WPA2PSK'
	option rts '2346'
	option IntraBSS '0'
	option frag '2346'
	option disabled '0'
	option WPACompatible '0'
	option shortgi '1'
	option group 'br-lan'
	option pmf '0'

config wifi-iface 'ath1'
