<Client>
	<DeviceId>
		<DisplayName>Roku Soundbridge</DisplayName>
		<DeviceDescriptionRecognition>
			<DescriptionText>SoundBridge</DescriptionText>
		</DeviceDescriptionRecognition>
		<MappingUpdatePolicy>FIX</MappingUpdatePolicy>
		<DeviceGroup>MUSIC</DeviceGroup>
	</DeviceId>
	<Capabilities>
		<MediaTypeList>
			<MediaType>MUSIC</MediaType>
		</MediaTypeList>
	</Capabilities>
	<Adaptations>
		<DLNA>
			<Version>DLNANO</Version>
		</DLNA>
		<HTTP>
			<HttpSetting>chunked</HttpSetting>
		</HTTP>
		<MimeTranslations>
			<MimeTypeMapping>
				<In>video/x-msvideo</In>
				<Out>video/avi</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>audio/L16</In>
				<Out>audio/lpcm</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>audio/mp1</In>
				<Out>audio/mpeg</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>audio/x-mpeg</In>
				<Out>audio/mpeg</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>application/ogg</In>
				<Out>audio/x-ogg</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>audio/x-wav</In>
				<Out>audio/wav</Out>
			</MimeTypeMapping>
		</MimeTranslations>
	</Adaptations>
	<DeviceQuirks>
		<Quirk>REDUCED_METADATA</Quirk>
		<Quirk>NOBITRATE</Quirk>
		<Quirk>ROKU_SEARCH</Quirk>
	</DeviceQuirks>
</Client>
