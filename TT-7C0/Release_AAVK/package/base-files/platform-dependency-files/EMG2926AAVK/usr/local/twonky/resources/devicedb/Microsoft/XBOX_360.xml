<Client>
	<DeviceId>
		<DisplayName>XBOX 360</DisplayName>
		<DeviceDescriptionRecognition>
			<DescriptionText>Xbox 360</DescriptionText>
		</DeviceDescriptionRecognition>
		<HttpRecognition>
			<HttpText>Xbox</HttpText>
		</HttpRecognition>
		<MappingUpdatePolicy>FIX</MappingUpdatePolicy>
	</DeviceId>
	<Capabilities>
		<Scaling>
			<Target>JPEG4096x4096</Target>
		</Scaling>
		<AudioTranscoding>
			<Target>LPCM</Target>
		</AudioTranscoding>
		<VideoTranscoding>
			<Source>video/x-matroska</Source>
			<Source>video/x-ms-dvr</Source>
			<Source>video/x-ms-wtv</Source>
			<Source>video/x-msvideo</Source>
			<Source>video/flv</Source>
			<Source>video/webm</Source>
			<Source>video/mpeg</Source>
			<Source>video/3gpp</Source>
			<Source>video/vnd.dlna.mpeg-tts</Source>
			<Target>MP4</Target>
			<Target>WMV</Target>
		</VideoTranscoding>
	</Capabilities>
	<Adaptations>
		<DLNA>
			<PretendServer>WMC</PretendServer>
			<Version>DLNA15</Version>
			<DescriptionFile>devicedescription-xbox.xml</DescriptionFile>
		</DLNA>
		<HTTP>
			<HttpSetting>chunked</HttpSetting>
		</HTTP>
		<MimeTranslations>
			<MimeTypeMapping>
				<In>audio/x-aiff</In>
				<Out>audio/aiff</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>video/x-msvideo</In>
				<Out>video/avi</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>audio/x-flac</In>
				<Out>audio/flac</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>audio/L16</In>
				<Out>audio/lpcm</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>audio/mp1</In>
				<Out>audio/mpeg</Out>
			</MimeTypeMapping>
			<MimeTypeMapping>
				<In>audio/x-mpeg</In>
				<Out>audio/mpeg</Out>
			</MimeTypeMapping>
				<MimeTypeSuppress>audio/x-wav</MimeTypeSuppress>
				<MimeTypeSuppress>video/x-matroska</MimeTypeSuppress>
				<MimeTypeSuppress>video/x-ms-dvr</MimeTypeSuppress>
				<MimeTypeSuppress>video/x-ms-wtv</MimeTypeSuppress>
				<MimeTypeSuppress>video/flv</MimeTypeSuppress>
				<MimeTypeSuppress>video/webm</MimeTypeSuppress>
				<MimeTypeSuppress>video/mpeg</MimeTypeSuppress>
				<MimeTypeSuppress>video/3gpp</MimeTypeSuppress>
				<MimeTypeSuppress>video/vnd.dlna.mpeg-tts</MimeTypeSuppress>
				<MimeTypeSuppress>image/png</MimeTypeSuppress>
				<MimeTypeSuppress>image/bmp</MimeTypeSuppress>
				<MimeTypeSuppress>image/gif</MimeTypeSuppress>
				<MimeTypeSuppress>image/tiff</MimeTypeSuppress>
		</MimeTranslations>
		<ExtraProtocolInfo>http-get:*:video/quicktime:*,http-get:*:video/x-msvideo:*</ExtraProtocolInfo>
	</Adaptations>
	<DeviceQuirks>
		<Quirk>FORCE_4TH_FIELD</Quirk>
		<Quirk>RES_ADD_FLAG</Quirk>
		<Quirk>UNLIMITED</Quirk>
		<Quirk>NEED_NO_PLAY_4_SETAVT</Quirk>
        <Quirk>EMPTY_SETAVT_4_FULL_STOP</Quirk>
        <Quirk>USEDIRECTURL</Quirk>
		<Quirk>STOP_WORKAROUND</Quirk>
	</DeviceQuirks>
</Client>
