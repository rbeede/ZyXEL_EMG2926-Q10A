# Base server settings
config proftpd global
       option 'port'			'21'
       option 'enable'			'0'
       option 'max_connection'  '10'
       option 'interface'		'0'

# Declare ftpusers
# Note: option usb1_rw and usb2_rw 
# 0:no access rights 1:read only 2: full access rights
config ftpuser profile1
       option 'name'			''
       option 'password'		''
       option 'enable'			'0'
       option 'usb1_rw'			'0'
       option 'usb2_rw'			'0'
       option 'downlo_speed'	''
       option 'uplo_speed'		''
	
