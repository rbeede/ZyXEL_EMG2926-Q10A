config interface interface
	option interface	'lan'
	option AdvSendAdvert	1
	option AdvManagedFlag	0
	option AdvOtherConfigFlag 1
	option ignore		0
	option MinRtrAdvInterval '60'
	option MaxRtrAdvInterval '120'

config prefix prefix
	option interface	'lan'
	option prefix		''
	option AdvOnLink	1
	option AdvAutonomous	1
	option AdvRouterAddr	0
	option Base6to4Interface ''
	option ignore		0
	option AdvPreferredLifetime '14400'
	option AdvValidLifetime '86400'

config route route
	option interface	'lan'
	option prefix		''
	option ignore		'0'
	option AdvRouteLifetime		'1122'
	option AdvRoutePreference	'high'

config rdnss rdnss
	option interface	'lan'
	option addr		''
	option ignore		0
